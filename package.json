{
	"name": "svelte-exmarkdown",
	"description": "Svelte component to render markdown. Dynamic and Extensible.",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && husky install",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test-watch": "vitest watch",
		"coverage": "vitest run --coverage",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"fmt": "prettier --write --plugin-search-dir=. .",
		"semantic-release": "semantic-release"
	},
	"peerDependencies": {
		"svelte": "^3.47.0 || ^4.0.0"
	},
	"dependencies": {
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"unified": "^10.1.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.2",
		"@commitlint/config-conventional": "^17.4.2",
		"@sveltejs/adapter-static": "^1.0.5",
		"@sveltejs/kit": "^1.3.7",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@testing-library/svelte": "^4.0.0",
		"@types/prismjs": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vitest/coverage-v8": "^0.32.0",
		"@vitest/ui": "^0.32.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.0",
		"jsdom": "^22.0.0",
		"mermaid": "^9.3.0",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"rehype-raw": "^6.1.1",
		"semantic-release": "^21.0.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte2tsx": "^0.6.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.0.4",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.32.0",
		"water.css": "^2.1.1"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ssssota/svelte-exmarkdown.git"
	},
	"homepage": "https://ssssota.github.io/svelte-exmarkdown",
	"keywords": [
		"svelte",
		"markdown",
		"remark",
		"unified",
		"commonmark",
		"gfm",
		"plugin"
	],
	"license": "MIT",
	"packageManager": "pnpm@8.6.6",
	"exports": {
		"./package.json": "./package.json",
		"./contexts": {
			"types": "./dist/contexts.d.ts",
			"default": "./dist/contexts.js"
		},
		"./ExtensibleSvelteMarkdown.svelte": {
			"types": "./dist/ExtensibleSvelteMarkdown.svelte.d.ts",
			"svelte": "./dist/ExtensibleSvelteMarkdown.svelte",
			"default": "./dist/ExtensibleSvelteMarkdown.svelte"
		},
		"./gfm": {
			"types": "./dist/gfm/index.d.ts",
			"svelte": "./dist/gfm/index.js",
			"default": "./dist/gfm/index.js"
		},
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./renderer/Children.svelte": {
			"types": "./dist/renderer/Children.svelte.d.ts",
			"svelte": "./dist/renderer/Children.svelte",
			"default": "./dist/renderer/Children.svelte"
		},
		"./renderer/html/Default.svelte": {
			"types": "./dist/renderer/html/Default.svelte.d.ts",
			"svelte": "./dist/renderer/html/Default.svelte",
			"default": "./dist/renderer/html/Default.svelte"
		},
		"./renderer/html": {
			"types": "./dist/renderer/html/index.d.ts",
			"svelte": "./dist/renderer/html/index.js",
			"default": "./dist/renderer/html/index.js"
		},
		"./renderer": {
			"types": "./dist/renderer/index.d.ts",
			"svelte": "./dist/renderer/index.js",
			"default": "./dist/renderer/index.js"
		},
		"./renderer/Renderer.svelte": {
			"types": "./dist/renderer/Renderer.svelte.d.ts",
			"svelte": "./dist/renderer/Renderer.svelte",
			"default": "./dist/renderer/Renderer.svelte"
		},
		"./renderer/Text.svelte": {
			"types": "./dist/renderer/Text.svelte.d.ts",
			"svelte": "./dist/renderer/Text.svelte",
			"default": "./dist/renderer/Text.svelte"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"default": "./dist/types.js"
		},
		"./utils": {
			"types": "./dist/utils.d.ts",
			"default": "./dist/utils.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"typesVersions": {
		">4.0": {
			"contexts": [
				"./dist/contexts.d.ts"
			],
			"ExtensibleSvelteMarkdown.svelte": [
				"./dist/ExtensibleSvelteMarkdown.svelte.d.ts"
			],
			"gfm": [
				"./dist/gfm/index.d.ts"
			],
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"renderer/Children.svelte": [
				"./dist/renderer/Children.svelte.d.ts"
			],
			"renderer/html/Default.svelte": [
				"./dist/renderer/html/Default.svelte.d.ts"
			],
			"renderer/html": [
				"./dist/renderer/html/index.d.ts"
			],
			"renderer": [
				"./dist/renderer/index.d.ts"
			],
			"renderer/Renderer.svelte": [
				"./dist/renderer/Renderer.svelte.d.ts"
			],
			"renderer/Text.svelte": [
				"./dist/renderer/Text.svelte.d.ts"
			],
			"types": [
				"./dist/types.d.ts"
			],
			"utils": [
				"./dist/utils.d.ts"
			]
		}
	}
}
